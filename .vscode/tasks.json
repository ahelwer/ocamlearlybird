{
	"version": "2.0.0",
	"tasks": [
		{
      "label": "build",
      "type": "shell",
      "command": "dune build src/main.bc",
      "group": {
        "isDefault": true,
        "kind": "build"
      }
    },
    {
      "label": "test",
      "type": "shell",
      "command": "dune exec src/main.bc -- serve --port=4712 --verbosity=debug",
      "group": {
        "isDefault": true,
        "kind": "test"
      }
		}
	]
}
