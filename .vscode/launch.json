  "version": "0.2.0",
  "configurations": [
    {
      "name": "OCaml",
      "type": "ocaml-debugger",
      "request": "launch",
      "program": "${workspaceRoot}/_build/default/src/main.bc",
      "console": "internalConsole",
      "arguments": [
        "serve"
      ],
      "stopOnEntry": true,
      "debugServer": 4712,
      // "env": {
      //   "OCAMLRUNPARAM": "b"
      // },
      "noDebug": false,
      "preLaunchTask": "build"
    }
  ]
}
